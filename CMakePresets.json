{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "DEBUG",
      "hidden": true,
      "generator": "Ninja",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "DEBUG": "ON"
      }
    },
    {
      "name": "RPG-DEBUG",
      "inherits": ["DEBUG"],
      "binaryDir": "build/RPG-DEBUG",
      "cacheVariables": {
        "GAME": "RPG"
      }
    },
    {
      "name": "MINE-DEBUG",
      "inherits": ["DEBUG"],
      "binaryDir": "build/MINE-DEBUG",
      "cacheVariables": {
        "GAME": "MINE"
      }
    },
    {
      "name": "CuZn-DEBUG",
      "inherits": ["DEBUG"],
      "binaryDir": "build/CuZn-DEBUG",
      "cacheVariables": {
        "GAME": "CuZn"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "RPG-libprotobuf",
      "configurePreset": "RPG-DEBUG",
      "targets": "libprotobuf"
    },
    {
      "name": "RPG-libSDL2",
      "configurePreset": "RPG-DEBUG",
      "targets": "libSDL2"
    },
    {
      "name": "RPG-libSDL2_image",
      "configurePreset": "RPG-DEBUG",
      "targets": "libSDL2_image"
    },
    {
      "name": "RPG-libSDL2_ttf",
      "configurePreset": "RPG-DEBUG",
      "targets": "libSDL2_ttf"
    },
    {
      "name": "RPG-libgtest",
      "configurePreset": "RPG-DEBUG",
      "targets": "libgtest"
    },

    {
      "name": "MINE-libprotobuf",
      "configurePreset": "MINE-DEBUG",
      "targets": "libprotobuf"
    },
    {
      "name": "MINE-libSDL2",
      "configurePreset": "MINE-DEBUG",
      "targets": "libSDL2"
    },
    {
      "name": "MINE-libSDL2_image",
      "configurePreset": "MINE-DEBUG",
      "targets": "libSDL2_image"
    },
    {
      "name": "MINE-libSDL2_ttf",
      "configurePreset": "MINE-DEBUG",
      "targets": "libSDL2_ttf"
    },
    {
      "name": "MINE-libgtest",
      "configurePreset": "MINE-DEBUG",
      "targets": "libgtest"
    },

    {
      "name": "CuZn-libprotobuf",
      "configurePreset": "CuZn-DEBUG",
      "targets": "libprotobuf"
    },
    {
      "name": "CuZn-libSDL2",
      "configurePreset": "CuZn-DEBUG",
      "targets": "libSDL2"
    },
    {
      "name": "CuZn-libSDL2_image",
      "configurePreset": "CuZn-DEBUG",
      "targets": "libSDL2_image"
    },
    {
      "name": "CuZn-libSDL2_ttf",
      "configurePreset": "CuZn-DEBUG",
      "targets": "libSDL2_ttf"
    },
    {
      "name": "CuZn-libgtest",
      "configurePreset": "CuZn-DEBUG",
      "targets": "libgtest"
    }
  ]
}